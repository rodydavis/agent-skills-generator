{
  "name": "agent-skills-generator",
  "displayName": "Agent Skills Generator",
  "description": "Generate Markdown documentation skills for AI agents by crawling websites.",
  "icon": "media/icon.png",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "keywords": [
    "ai",
    "documentation",
    "crawler",
    "markdown",
    "skills",
    "agent"
  ],
  "categories": [
    "Other"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/rodydavis/agent-skills-generator.git"
  },
  "homepage": "https://github.com/rodydavis/agent-skills-generator/blob/main/vscode-extension/README.md",
  "bugs": {
    "url": "https://github.com/rodydavis/agent-skills-generator/issues"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "agent-skills-sidebar",
          "title": "Agent Skills",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "agent-skills-sidebar": [
        {
          "id": "agent-skills-view",
          "name": "Skills Generator",
          "type": "webview"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/turndown": "^5.0.4",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "js-yaml": "^4.1.0",
    "sharp": "^0.34.5",
    "turndown": "^7.1.3"
  }
}